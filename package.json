{
  "name": "preact-aria-bootstrap",
  "type": "module",
  "version": "0.0.1",
  "description": "Preact implementation of Bootstrap with better accessability",
  "source": "src/index.tsx",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "npx npm-run-all --npm-path npm -pc build:*",
    "watch": "npx npm-run-all --npm-path npm -pc watch:*",
    "watch:ts": "tsc --watch",
    "build:ts": "tsc",
    "test": "jest",
    "lint": "npx lint ./src",
    "prepare": "npm run build || node ./true",
    "clean": "tsc --build --clean"
