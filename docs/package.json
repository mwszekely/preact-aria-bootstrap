{
    "name": "preact-aria-widgets-demo",
    "version": "0.0.1",
    "description": "A small set of hooks and other functions related to modifying Preact props.",
    "main": "index.tsx",
    "packageManager": "pnpm@8.5.1",
    "browserslist": "defaults, iOS >= 12",
    "scripts": {
        "build": "npx npm-run-all --npm-path npm -sc build:js build:sass build:css",
        "watch": "npx npm-run-all --npm-path npm -pc watch:*",
        "watch:js": "rollup --config rollup.config.mjs --watch --no-watch.clearScreen",
        "build:js": "rollup --config rollup.config.mjs",
        "watch:sass": "sass --watch --load-path=\"./node_modules/\" ../src/index.scss ./index-unprefixed.css",
        "watch:css": "postcss ./index-unprefixed.css --use autoprefixer --watch -o ./index.css",
        "build:sass": "sass --load-path=\"./node_modules/\" ../src/index.scss ./index-unprefixed.css",
        "build:css": "postcss ./index-unprefixed.css --use autoprefixer -o ./index.css",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.21.8",
        "@babel/preset-env": "^7.21.5",
        "@rollup/plugin-alias": "^5.0.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.1.0",
        "@rollup/plugin-node-resolve": "^15.0.2",
        "@rollup/plugin-replace": "^5.0.2",
        "@rollup/plugin-typescript": "^11.1.0",
        "autoprefixer": "^10.4.14",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.23",
        "postcss-cli": "^10.1.0",
        "rollup": "^3.21.3",
        "rollup-plugin-sourcemaps": "^0.6.3",
        "sass": "^1.62.1",
        "serve": "^14.2.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "bootstrap": "5.3.0-alpha3",
        "bootstrap-sass-variables": "github:mwszekely/bootstrap-sass-variables",
        "clsx": "^1.2.1",
        "preact": "^10.13.2",
        "preact-transition": "github:mwszekely/preact-transition",
        "tslib": "^2.5.0"
    }
}
