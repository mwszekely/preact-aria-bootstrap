{
  "name": "tests",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx npm-run-all --npm-path npm -pc build:sass build:js",
    "watch": "npx npm-run-all --npm-path npm -s build watch-impl",
    "watch-impl": "npx npm-run-all --npm-path npm -p watch:*",
    "build:sass": "sass --load-path=\"./node_modules\" ./test-source/index.scss:./test-source/index.css",
    "build:js": "rollup --config test-source/rollup.config.js",
    "watch:sass": "sass --load-path=\"./node_modules\" ./test-source/index.scss:./test-source/index.css --watch",
    "watch:js": "rollup --config test-source/rollup.config.js --watch --no-watch.clearScreen",
    "serve": "npx serve ..",
    "start": "npx npm-run-all --npm-path npm -p watch serve"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@axe-core/playwright": "^4.7.0",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "rollup": "^3.21.5",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "serve": "^14.2.0",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.0",
    "esbuild": "^0.17.18",
    "preact-hash-router": "github:mwszekely/preact-hash-router"
  }
}